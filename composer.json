{
    "name": "chimera/bus-tactician",
    "description": "Service bus adapter for league/tactician",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "chimera/foundation": "^0.4@dev",
        "league/tactician": "^1.0",
        "league/tactician-container": "^2.0"
    },
    "require-dev": {
        "infection/infection": "^0.16",
        "lcobucci/coding-standard": "^5.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^9.2"
    },
    "provide": {
        "chimera/bus-implementation": "0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Chimera\\ServiceBus\\Tactician\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chimera\\ServiceBus\\Tactician\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
