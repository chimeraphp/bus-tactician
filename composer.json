{
    "name": "chimera/bus-tactician",
    "description": "Service bus adapter for league/tactician",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "chimera/foundation": "^0.3",
        "league/tactician": "^1.0",
        "league/tactician-container": "^2.0"
    },
    "require-dev": {
        "infection/infection": "^0.15.1",
        "lcobucci/coding-standard": "^3.1",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-deprecation-rules": "^0.11",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Chimera\\ServiceBus\\Tactician\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chimera\\ServiceBus\\Tactician\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
